{
  "name": "@snapboard/core",
  "version": "0.7.0",
  "description": "A library for interacting with Snapboard",
  "main": "./dist/index.js",
  "module": "./dist/es/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:library",
    "build:library": "./scripts/build-library.js",
    "build:package": "./scripts/build-package.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:debug": "node --inspect --inspect-brk node_modules/.bin/jest",
    "test:watch": "yarn test -- --watch",
    "version": "yarn build",
    "release": "npm run build && npm run build:package && ./scripts/release.js",
    "postversion": "git push",
    "postpublish": "git push --tags",
    "coveralls": "NODE_ENV=spec nyc yarn test && nyc report --reporter=text-lcov | coveralls",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-react-app": "^9.0.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "eslint-plugin-standard": "^4.0.0",
    "inquirer": "^6.4.1",
    "jest": "^24.8.0",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^13.2.4"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {}
}
